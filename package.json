{
  "name": "js-ini-parser",
  "version": "1.1.0",
  "description": "A simple ini parser that save comments and spaces",
  "author": "thegostisdead@gmail.com",
  "homepage": "https://github.com/thegostisdead/js-ini-parser#readme",
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts src && prettier -c src test",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && pnpm vitest run",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "changelogen": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "unbuild": "^1.2.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "license": "MIT",
  "keywords": [
    "ini",
    "parser",
    "config",
    "ini-parser",
    "ini-parser-node",
    "ini-parse",
    "typescript",
    "simple",
    "javascript",
    "client"
  ],
  "packageManager": "pnpm@7.32.2",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}